  "events": {
    "load": [
      {
        "error": [],
        "name": "MonthlyStats",
        "schema": {
          "$ref": "#/definitions/Stats",
          "dotnetType": "RadzenCrm.Pages.Stats"
        },
        "then": [
          {
            "name": "monthlyStats",
            "type": "set",
            "value": "${result}",
            "valueType": "RadzenCrm.Pages.Stats"
          }
        ],
        "type": "invoke-method"
      },
      {
        "name": "RevenueByCompany",
        "schema": {
          "dotnetType": "System.Collections.Generic.IEnumerable<RadzenCrm.Pages.RevenueByCompany>",
          "items": {
            "$ref": "#/definitions/RevenueByCompany",
            "dotnetType": "RadzenCrm.Pages.RevenueByCompany"
          },
          "type": "array"
        },
        "then": [
          {
            "name": "revenueByCompany",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenCrm.Pages.RevenueByCompany>"
          }
        ],
        "type": "invoke-method"
      },
      {
        "name": "RevenueByEmployee",
        "schema": {
          "dotnetType": "System.Collections.Generic.IEnumerable<RadzenCrm.Pages.RevenueByEmployee>",
          "items": {
            "$ref": "#/definitions/RevenueByEmployee",
            "dotnetType": "RadzenCrm.Pages.RevenueByEmployee"
          },
          "type": "array"
        },
        "then": [
          {
            "name": "revenueByEmployee",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenCrm.Pages.RevenueByEmployee>"
          }
        ],
        "type": "invoke-method"
      },
      {
        "name": "RevenueByMonth",
        "schema": {
          "dotnetType": "System.Collections.Generic.IEnumerable<RadzenCrm.Pages.RevenueByMonth>",
          "items": {
            "$ref": "#/definitions/RevenueByMonth",
            "dotnetType": "RadzenCrm.Pages.RevenueByMonth"
          },
          "type": "array"
        },
        "then": [
          {
            "name": "revenueByMonth",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenCrm.Pages.RevenueByMonth>"
          }
        ],
        "type": "invoke-method"
      },
      {
        "name": "CRM.getOpportunities",
        "parameters": [
          {
            "name": "$orderby",
            "value": [
              {
                "direction": "desc",
                "property": "CloseDate"
              }
            ]
          }
        ],
        "then": [
          {
            "name": "getOpportunitiesResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenCrm.Models.Crm.Opportunity>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "CRM.getTasks",
        "parameters": [
          {
            "name": "$orderby",
            "value": [
              {
                "direction": "desc",
                "property": "DueDate"
              }
            ]
          }
        ],
        "then": [
          {
            "name": "getTasksResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenCrm.Models.Crm.Task>"
          }
        ],
        "type": "invoke"
      }
    ]
